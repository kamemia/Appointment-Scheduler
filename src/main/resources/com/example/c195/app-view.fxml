<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.shape.Line?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.c195.HelloController">
    <Label fx:id="label" text="Appointments">
    </Label>
    <TableView fx:id="appTable">
        <columns>
            <TableColumn fx:id="IdCol" text="ID"/>
            <TableColumn fx:id="TitleCol" text="Title"/>
            <TableColumn fx:id="TypeCol" text="Type"/>
            <TableColumn fx:id="DespcriptionCol" text="Description"/>
            <TableColumn fx:id="LocationCol" text="Location"/>
            <TableColumn fx:id="StartDateCol" text="Start Date/Time"/>
            <TableColumn fx:id="EndDateCol" text="Last Date/Time"/>
            <TableColumn fx:id="ContactCol" text="Contact"/>
            <TableColumn fx:id="CustomerIdCol" text="Customer ID"/>
            <TableColumn fx:id="UserIdCol" text="User ID"/>
        </columns>
    </TableView>
    <Button text="Add">
    </Button>
    <Button text="Update">
    </Button>
    <Button text="Delete">
    </Button>
    <Line layoutX="50.0" layoutY="50.0" endX="100" />
    <Label fx:id="customers" text="Customers">
    </Label>


</VBox>
